{
  "compilerOptions": {
    "outDir": "./dist/",
    "declaration": true,
    "declarationDir": "./dist/",
    "emitDeclarationOnly": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "typeRoots": ["contextCursor/types", "node_modules/@types"],
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
